(function () {
  function isMobileOrTablet() {
    var ua = navigator.userAgent || "";
    return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(ua);
  }
  if (!isMobileOrTablet()) {
    window.open('', '_self');
    window.close();
    document.body.innerHTML = "";
  }
})();
